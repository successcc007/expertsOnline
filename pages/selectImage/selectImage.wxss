page{
  background: #000;
}
.selectImage{
  width: 95%;
  height: 800rpx;
  margin: 70rpx auto;
  position: relative;
}
.selectImage image{
  width: 100%;
  height: 100%;
}
.selectImage .tip{
  width:400rpx;
  height:80rpx;
  line-height:80rpx;
  color:#fff;
  background-color:#000;
  opacity:0.3;
  filter:alpha(opacity=30);
  position:absolute;
  top:300rpx;
  left:24%;
  text-align:center;
  font-size:34rpx;
}
.photo-wrap{
  width: 100%;
  height:170rpx;
  position: relative;
}
.photo-image-wrap{
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  text-align: center;
}
.photo-image-wrap image{
  width: 100%;
  height: 100%;
}
.photo-image-wrap text{
  color: #fff;
  font-size: 30rpx;
}
.photo-image-wrap1{
  top: 10rpx;
  left: 24%;
}
.photo-image-wrap2{
  top: 10rpx;
  left: 60%;
}